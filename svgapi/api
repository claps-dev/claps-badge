package main

import (
	"fmt"
	"net/http"
	"strconv"
	_ "strings"
)

func handle(w http.ResponseWriter, r *http.Request) {
	info := make(map[string][2] string)
	fmt.Println("有人访问了")
	for true  {
		info=message()
		if len(info)>0{
			fmt.Println(info)
			break
		}
	}
	r.ParseForm()
	w.Header().Add("Access-Control-Allow-Origin", "*")//允许跨域请求
	w.Header().Set("Content-Type", "image/svg+xml")
	var x string
	var a[2] string
	var money_kind string
	var color1 string
	var size1 string
	var bgcolor1 string
	var total_donation string
	var oo int//第一种svg中圆角矩形的宽度
    var fiats float64
	var total_money float64
	name,_:=r.Form["project"]
	money,_:=r.Form["money"]
	color,_:=r.Form["color"]
	bgcolor,_:=r.Form["bgcolor"]
	size,_:=r.Form["size"]
	if len(name)>=1{
		x=name[0]
	}
	if len(money)>=1{
		money_kind =money[0]
	}
	if len(color)>=1{
		color1 =color[0]
		fmt.Println(color1)
	}
	if len(bgcolor)>=1{
		bgcolor1 =bgcolor[0]
		fmt.Println(bgcolor1)
	}
	if len(size)>=1{
		size1 =size[0]
	}
	if _,ok:=info[x];ok{
		fmt.Println(x,"yes")
		a=info[x]
		fiats=fiat(money_kind)//货币对于美元的汇率
		if fiats==0{
			w.WriteHeader(400)
			return
		}
		total_money,_=strconv.ParseFloat(a[1],64)
		total_donation = strconv.FormatFloat(total_money*fiats,'f',2,64)+"  "+ money_kind
	}
	if _,ok:=info[x];!ok{
		w.WriteHeader(400)
		return
	}
	oo=10*len(a[0])+150
	fmt.Println(oo)
	if size1 =="0"{
		fmt.Fprintf(w, "<svg xmlns=\"http://www.w3.org/2000/svg\" version=\"1.1\">\n")
		fmt.Fprintf(w, "<a href=\"https://claps.dev/projects/%s\" title=\"a\" target=\"_blank\">\n",a[0])
